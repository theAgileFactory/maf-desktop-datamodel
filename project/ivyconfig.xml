<ivysettings>
    <property name='ivy.checksums' value=''/>
    <statuses>
        <status name='integration' integration='true'/>
    </statuses>
    <resolvers>
        <url name='deadbolt-releases'>
            <ivy      pattern='http://schaloner.github.io/releases/be.objectify/[artifact]/[revision]/ivys/ivy.xml'/>
            <artifact pattern='http://schaloner.github.io/releases/be.objectify/[artifact]/[revision]/jars/[artifact](.[ext])'/>
        </url>
    </resolvers>
    <modules>
        <module organisation='be.objectify' name='deadbolt*'  matcher="glob" resolver='deadbolt-releases'/>
    </modules>
</ivysettings>
